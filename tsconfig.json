{
    "compilerOptions": {
      "outDir": "./dist",
      "target": "es5",
      "module": "esnext",
      "moduleResolution": "node",
      "jsx": "react",
      "lib": ["dom", "esnext"],
      "noImplicitAny": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "baseUrl": "./src",
    },
    "include": ["./src/"],
    "exclude": ["node_modules,", "build"]
  }